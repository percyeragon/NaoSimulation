BINFILE = naoSimTest
BINFILEIA = naoInteractive

interactive: $(TARGETS) ./naoSimInteractive.o
	$(LINK) -o $(BINFILEIA) $(TARGETS) ./naoSimInteractive.o $(LIBS)

test: $(TARGETS) ./testNaoSim.o
	$(LINK) -o $(BINFILE) $(TARGETS) ./testNaoSim.o $(LIBS)

%.o: %.cpp
	$(CXX) $(CXX_FLAGS) $(ODE_FLAGS) $(INCLUDE_DIRS) -c $< -o $@

clean:
	rm -f $(TARGETS) ./naoSimInteractive.o ./testNaoSim.o 
	rm -f $(BINFILE) $(BINFILEIA) $(BINFILETEST)
	
interaktiv:	
	make
	make interactive
	./naoInteractive
Tests:	
	make
	make test
	./naoSimTest
.PHONY: clean
